 <%= render 'layouts/error_messages', object: f.object %>

 <div class="form-group">
    <%= f.label :artist, style: "display: block" %>
    <%= f.select :artist, @list_of_artists.collect {|a| [ a.name, a.id ] }, {}, class: 'form-control' %>
 </div>

 <div class="form-group">
  <%= f.label :album_name, style: "display: block" %>
  <%= f.text_field :album_name, class: 'form-control'  %>
 </div>

<div class="form-group">
  <%= f.label :year, style: "display: block" %>
  <%= f.number_field :year, class: 'form-control' %>
</div>